container_commands:
  01_npm_install:
    command: "npm install"
    leader_only: true
  02_webpack:
    command: "./node_modules/.bin/webpack --config webpack.config.js"
    leader_only: true
  03_collectstatic:
    command: "python manage.py collectstatic --noinput"
    leader_only: true
  04_migrate:
    command: "python manage.py migrate --noinput"
    leader_only: true
